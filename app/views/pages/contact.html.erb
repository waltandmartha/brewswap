<h1>Pages#contact</h1>
<p>Find me in app/views/pages/contact.html.erb</p>

<%= form_for :contact do |f| %>
    <p>
        <%= f.label :name %><br/>
        <%= f.text_field :name %>
    </p>

    <p>
        <%= f.label :message %><br/>
        <%= f.text_area :message %>
    </p>

    <%= f.submit %>
<% end %>
